CREATE TABLE PRICE(
PRICE_CODE	INTEGER	NOT NULL	PRIMARY KEY,
PRICE_DESCRIPTION	CHAR(20)	NOT NULL,
PRICE_RENTFEE	DECIMAL	NOT NULL,
PRICE_DAILYLATEFEE	DECIMAL	NOT NULL);

INSERT INTO PRICE
	VALUES (1,'Standard',2,1)
INSERT INTO PRICE
	VALUES (2,'New Release',3.5,3)
INSERT INTO PRICE
	VALUES (3,'Discount',1.5,1)
INSERT INTO PRICE
	VALUES (4,'Weekly Special',1,.5)

COMMIT;

UPDATE 	MOVIE
SET	MOVIE_YEAR = '2006'
WHERE	MOVIE_NUM = '1246';

UPDATE	MOVIE
SET	PRICE_CODE = 3
WHERE MOVIE_GENRE = 'ACTION';

UPDATE	PRICE
SET	PRICE_RENTFEE = PRICE_RENTFEE + .50
WHERE	PRICE_RENTFEE NOT NULL;

COMMIT;

SELECT	MOVIE_NAME, MOVIE_YEAR, MOVIE_GENRE
FROM		MOVIE

SELECT MOVIE_YEAR, MOVIE_NAME, MOVIE_COST
FROM		MOVIE
GROUP BY	(MOVIE_YEAR) DESC;

SELECT MOVIE_NAME, MOVIE_YEAR, MOVIE_GENRE
FROM		MOVIE
ORDER BY	MOVIE_GENRE, MOVIE_YRAR DESC;

SELECT	MOVIE_NUM, MOVIE_NAME, PRICE_CODE
FROM		MOVIE
WHERE	MOVIE_NAME LIKE 'R%';

SELECT	MOVIE_NAME, MOVIE_YEAR, MOVIE_COST
FROM 	MOVIE
WHERE	UPPER (MOVIE_NAME) LIKE 'hope'
ORDER BY	MOVIE_NAME;

SELECT	MOVIE_NAME, MOVIE_YEAR, MOVIE_GENRE
FROM		MOVIE
WHERE 	MOVIE_GENRE = 'ACTION';

SELECT	MOVIE_NUM, MOVIE_NAME, MOVIE_COST
FROM		MOVIE
WHERE	MOVIE_COST <  40;

SELECT	MOVIE_NUM, MOVIE_NAME, MOVIE_COST, MOVIE_GENRE
FROM		MOVIE	
WHERE	MOVIE_GENRE = 'ACTION' 
		OR MOVIE_GENRE = 'COMEDY'
		AND MOVIE_COST < 50.00
ORDER BY	GENRE;

ALTER TALBE MOVIE
	ADD (MOVIE_DESCIPTION CHAR(55));
