CREATE TABLE MEMBERSHIP(
MEM_NUM	NUMBER	NOT NULL	PRIMARY KEY,
MEM_NAME VARCHAR(15)	NOT NULL,
MEM_LNAME	VARCHAR(15)	NOT NULL,
MEM_STREET	CHAR(25)	NOT NULL,
MEM_CITY		CHAR(25)	NOT NULL,
MEM_STATE	CHAR(2)	NOT NULL,
MEM_ZIP		INTEGER	NOT NULL,
MEM_BALANCE	INTEGER	NOT NULL);

