CREATE TABLE MOVIE(
MOVIE_NUM	INTEGER	NOT NULL	PRIMARY KEY,
MOVIE_NAME	CHAR(35)	NOT NULL,
MOVIE_YEAR	INTEGER,
MOVIE_COST	DECIMAL	NOT NULL,
MOVIE_GENRE	CHAR(10)	NOT NULL,
PRICE_CODE	INTEGER	NOT NULL, FOREIGN KEY);

